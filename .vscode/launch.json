{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome against localhost, with sourcemaps",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200/",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}/fint-kunde-portal-frontend/client",
      "port": 9223,
      "userDataDir": "${workspaceRoot}/out/chrome",
      "sourceMapPathOverrides": {
        "webpack:///./*": "${workspaceRoot}/fint-kunde-portal-frontend/*"
      }
    }
  ]
}
